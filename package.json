{
	"name": "github-punchcard",
	"private": false,
	"version": "1.0.0",
	"type": "module",
	"author": {
		"name": "vnau"
	},
	"description": "GitHub PunchCard shows commit activity by day and hour in a clean, shareable heatmap.",
	"homepage": "https://github.com/vnau/punchcard",
	"license": "MIT",
	"keywords": [
		"github punchcard",
		"commit heatmap",
		"commit visualization",
		"developer analytics"
	],
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^6.2.5"
	},
	"dependencies": {
		"html2canvas": "^1.4.1",
		"svelte-tags-input": "^6.0.2"
	}
}